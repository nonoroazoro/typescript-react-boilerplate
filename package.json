{
    "name": "typescript-react-boilerplate",
    "version": "1.0.0",
    "description": "Minimal example for React with TypeScript.",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "author": {
        "email": "xiaochao.k@gmail.com",
        "name": "nonoroazoro",
        "url": "https://github.com/nonoroazoro"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nonoroazoro/typescript-react-boilerplate"
    },
    "homepage": "https://github.com/nonoroazoro/typescript-react-boilerplate",
    "bugs": {
        "url": "https://github.com/nonoroazoro/typescript-react-boilerplate/issues"
    },
    "keywords": [
        "boilerplate",
        "react",
        "typescript-react-boilerplate",
        "typescript",
        "typescript-eslint"
    ],
    "scripts": {
        "prebuild": "npm run clean && npm run cleanCov",
        "clean": "rimraf dist/*",
        "cleanCov": "rimraf @coverage",
        "start": "npm run dev",
        "dev": "node ./scripts/server/devServer",
        "build": "npm run build:prod && npm run build:es",
        "build:es": "tsc --project ./tsconfig.es.json",
        "build:prod": "cross-env NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.prod.js",
        "build:analyze": "cross-env NODE_ENV=production BUILD_ANALYZE=true webpack --config ./scripts/webpack/webpack.config.prod.js --env.analyze",
        "test": "jest --config ./scripts/jest/jest.config.json",
        "test:watch": "npm test -- --watch",
        "test:cov": "npm run cleanCov && npm test -- --coverage",
        "test:update": "npm test -- --updateSnapshot"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "history": "^4.10.1",
        "normalize.css": "^8.0.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-fxxking-hooks": "^1.0.2",
        "react-router-dom": "^5.1.2"
    },
    "devDependencies": {
        "@hot-loader/react-dom": "^16.13.0",
        "@types/classnames": "^2.2.10",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^25.1.4",
        "@types/react": "^16.9.29",
        "@types/react-dom": "^16.9.5",
        "@types/react-router-dom": "^5.1.3",
        "@typescript-eslint/eslint-plugin": "^2.26.0",
        "@typescript-eslint/parser": "^2.26.0",
        "cache-loader": "^4.1.0",
        "chalk": "^3.0.0",
        "cross-env": "^7.0.2",
        "css-loader": "^3.4.2",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.4.4",
        "eslint": "^6.8.0",
        "eslint-config-zoro": "^2.8.1",
        "eslint-loader": "^3.0.3",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^3.0.0",
        "file-loader": "^6.0.0",
        "husky": "^4.2.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^25.2.4",
        "jest-fetch-mock": "^3.0.3",
        "less": "^3.11.1",
        "less-loader": "^5.0.0",
        "lint-staged": "^10.1.0",
        "log-symbols": "^3.0.0",
        "mini-css-extract-plugin": "^0.9.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "react-hot-loader": "^4.12.20",
        "rimraf": "^3.0.2",
        "style-loader": "^1.1.3",
        "terser-webpack-plugin": "^2.3.5",
        "ts-jest": "^25.3.0",
        "ts-loader": "^6.2.2",
        "tslib": "^1.11.1",
        "typescript": "^3.8.3",
        "url-loader": "^4.0.0",
        "webpack": "^4.42.1",
        "webpack-bundle-analyzer": "^3.6.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpackbar": "^4.0.0"
    }
}
