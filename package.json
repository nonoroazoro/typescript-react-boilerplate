{
    "name": "typescript-react-boilerplate",
    "version": "1.0.0",
    "description": "Minimal example for React with TypeScript.",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/es/index.js",
    "types": "dist/types/index.d.ts",
    "author": {
        "email": "xiaochao.k@gmail.com",
        "name": "nonoroazoro",
        "url": "https://github.com/nonoroazoro"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nonoroazoro/typescript-react-boilerplate"
    },
    "homepage": "https://github.com/nonoroazoro/typescript-react-boilerplate",
    "bugs": {
        "url": "https://github.com/nonoroazoro/typescript-react-boilerplate/issues"
    },
    "keywords": [
        "boilerplate",
        "react",
        "typescript-react-boilerplate",
        "typescript",
        "typescript-eslint"
    ],
    "scripts": {
        "prepare": "husky install",
        "prebuild": "npm run clean && npm run cleanCov",
        "clean": "rimraf dist/*",
        "cleanCov": "rimraf @coverage",
        "start": "npm run dev",
        "dev": "node ./scripts/server/devServer",
        "build": "npm run build:es && npm run build:types && npm run build:prod",
        "build:types": "tsc --project ./tsconfig.types.json",
        "build:es": "tsc --project ./tsconfig.es.json",
        "build:prod": "cross-env NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.prod.js",
        "build:analyze": "cross-env NODE_ENV=production BUILD_ANALYZE=true webpack --config ./scripts/webpack/webpack.config.prod.js --env.analyze",
        "test": "jest --config ./scripts/jest/jest.config.json",
        "test:watch": "npm test -- --watch",
        "test:cov": "npm run cleanCov && npm test -- --coverage",
        "test:update": "npm test -- --updateSnapshot",
        "upgradepackages": "./scripts/upgrade_packages.sh"
    },
    "dependencies": {
        "classnames": "^2.3.1",
        "memoize-one": "^6.0.0",
        "normalize.css": "^8.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-fxxking-hooks": "^1.2.3",
        "react-router-dom": "^6.1.1"
    },
    "devDependencies": {
        "@hot-loader/react-dom": "^17.0.1",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@types/jest": "^27.0.3",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.2",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "chalk": "^4.1.2",
        "cross-env": "^7.0.3",
        "css-loader": "^6.5.1",
        "css-minimizer-webpack-plugin": "^3.2.0",
        "eslint": "^8.4.1",
        "eslint-config-zoro": "^5.4.0",
        "eslint-plugin-react": "^7.27.1",
        "eslint-plugin-react-hooks": "^4.3.0",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^6.5.0",
        "husky": "^7.0.4",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.4.5",
        "less": "^4.1.2",
        "less-loader": "^10.2.0",
        "lint-staged": "^12.1.2",
        "mini-css-extract-plugin": "^2.4.5",
        "react-hot-loader": "^4.13.0",
        "react-test-renderer": "^17.0.2",
        "rimraf": "^3.0.2",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.2.5",
        "ts-jest": "^27.1.1",
        "ts-loader": "^9.2.6",
        "tslib": "^2.3.1",
        "typescript": "^4.5.4",
        "url-loader": "^4.1.1",
        "webpack": "^5.65.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.6.0",
        "webpackbar": "^5.0.2"
    }
}
