{
    "name": "typescript-react-boilerplate",
    "version": "1.0.0",
    "description": "Minimal example for React with TypeScript.",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/es/index.js",
    "types": "dist/types/index.d.ts",
    "author": {
        "email": "xiaochao.k@gmail.com",
        "name": "nonoroazoro",
        "url": "https://github.com/nonoroazoro"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nonoroazoro/typescript-react-boilerplate"
    },
    "homepage": "https://github.com/nonoroazoro/typescript-react-boilerplate",
    "bugs": {
        "url": "https://github.com/nonoroazoro/typescript-react-boilerplate/issues"
    },
    "keywords": [
        "boilerplate",
        "react",
        "typescript-react-boilerplate",
        "typescript",
        "typescript-eslint"
    ],
    "engines": {
        "node": ">=12.0.0"
    },
    "scripts": {
        "prepare": "husky install",
        "prebuild": "npm run clean && npm run cleanCov",
        "clean": "rimraf dist/*",
        "cleanCov": "rimraf @coverage",
        "start": "npm run dev",
        "dev": "node ./scripts/server/devServer",
        "build": "npm run build:es && npm run build:types && npm run build:prod",
        "build:types": "tsc --project ./tsconfig.types.json",
        "build:es": "tsc --project ./tsconfig.es.json",
        "build:prod": "cross-env NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.prod.js",
        "build:analyze": "cross-env NODE_ENV=production BUILD_ANALYZE=true webpack --config ./scripts/webpack/webpack.config.prod.js --env.analyze",
        "test": "jest --config ./scripts/jest/jest.config.json",
        "test:watch": "npm test -- --watch",
        "test:cov": "npm run cleanCov && npm test -- --coverage",
        "test:update": "npm test -- --updateSnapshot",
        "upgradepackages": "./scripts/upgrade_packages.sh"
    },
    "dependencies": {
        "classnames": "^2.3.2",
        "memoize-one": "^6.0.0",
        "normalize.css": "^8.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-fxxking-hooks": "^1.2.3",
        "react-router-dom": "^6.4.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/jest": "^29.2.3",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "chalk": "^4.1.2",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.2",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "eslint": "^8.27.0",
        "eslint-config-zoro": "^5.34.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-webpack-plugin": "^3.2.0",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^7.2.13",
        "husky": "^8.0.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.3.1",
        "less": "^4.1.3",
        "less-loader": "^11.1.0",
        "lint-staged": "^13.0.3",
        "mini-css-extract-plugin": "^2.6.1",
        "react-hot-loader": "^4.13.1",
        "rimraf": "^3.0.2",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.3.6",
        "ts-jest": "^29.0.3",
        "ts-loader": "^9.4.1",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.75.0",
        "webpack-bundle-analyzer": "^4.7.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1",
        "webpackbar": "^5.0.2"
    }
}
