{
    "name": "typescript-react-boilerplate",
    "version": "1.0.0",
    "description": "Minimal example for React with TypeScript.",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "author": {
        "email": "xiaochao.k@gmail.com",
        "name": "nonoroazoro",
        "url": "https://github.com/nonoroazoro"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nonoroazoro/typescript-react-boilerplate"
    },
    "homepage": "https://github.com/nonoroazoro/typescript-react-boilerplate",
    "bugs": {
        "url": "https://github.com/nonoroazoro/typescript-react-boilerplate/issues"
    },
    "keywords": [
        "boilerplate",
        "react",
        "typescript-react-boilerplate",
        "typescript",
        "typescript-eslint"
    ],
    "scripts": {
        "prebuild": "npm run clean && npm run cleanCov",
        "clean": "rimraf dist/*",
        "cleanCov": "rimraf @coverage",
        "start": "npm run dev",
        "dev": "node ./scripts/server/devServer",
        "build": "npm run build:prod && npm run build:es",
        "build:es": "tsc --project ./tsconfig.es.json",
        "build:prod": "cross-env NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.prod.js",
        "build:analyze": "cross-env NODE_ENV=production BUILD_ANALYZE=true webpack --config ./scripts/webpack/webpack.config.prod.js --env.analyze",
        "test": "jest --config ./scripts/jest/jest.config.js",
        "test:watch": "npm test -- --watch",
        "test:cov": "npm run cleanCov && npm test -- --coverage",
        "test:update": "npm test -- --updateSnapshot"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "history": "^4.10.1",
        "normalize.css": "^8.0.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-fxxking-hooks": "^1.0.2",
        "react-router-dom": "^5.1.2"
    },
    "devDependencies": {
        "@hot-loader/react-dom": "^16.11.0",
        "@types/classnames": "^2.2.9",
        "@types/enzyme": "^3.10.3",
        "@types/jest": "^24.0.23",
        "@types/react": "^16.9.15",
        "@types/react-dom": "^16.9.4",
        "@types/react-router-dom": "^5.1.3",
        "@typescript-eslint/eslint-plugin": "^2.10.0",
        "@typescript-eslint/parser": "^2.10.0",
        "cache-loader": "^4.1.0",
        "chalk": "^3.0.0",
        "cross-env": "^6.0.3",
        "css-loader": "^3.2.1",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.15.1",
        "enzyme-to-json": "^3.4.3",
        "eslint": "^6.7.2",
        "eslint-config-zoro": "^2.7.3",
        "eslint-loader": "^3.0.2",
        "eslint-plugin-react": "^7.17.0",
        "eslint-plugin-react-hooks": "^2.3.0",
        "file-loader": "^5.0.2",
        "husky": "^3.1.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.9.0",
        "jest-fetch-mock": "^2.1.2",
        "less": "^3.10.3",
        "less-loader": "^5.0.0",
        "lint-staged": "^9.5.0",
        "log-symbols": "^3.0.0",
        "mini-css-extract-plugin": "^0.8.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "react-hot-loader": "^4.12.18",
        "rimraf": "^3.0.0",
        "style-loader": "^1.0.1",
        "terser-webpack-plugin": "^2.2.1",
        "ts-jest": "^24.2.0",
        "ts-loader": "^6.2.1",
        "tslib": "^1.10.0",
        "typescript": "^3.7.3",
        "url-loader": "^3.0.0",
        "webpack": "^4.41.2",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.10",
        "webpack-dev-server": "^3.9.0",
        "webpackbar": "^4.0.0"
    }
}
