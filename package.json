{
    "name": "typescript-react-boilerplate",
    "version": "1.0.0",
    "description": "Minimal example for React with TypeScript.",
    "license": "MIT",
    "main": "dist/typescript-react-boilerplate.min.js",
    "types": "dist/types/index.d.ts",
    "author": {
        "email": "xiaochao.k@gmail.com",
        "name": "nonoroazoro",
        "url": "http://www.alibabagroup.com/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/nonoroazoro/typescript-react-boilerplate"
    },
    "homepage": "https://github.com/nonoroazoro/typescript-react-boilerplate",
    "bugs": {
        "url": "https://github.com/nonoroazoro/typescript-react-boilerplate/issues"
    },
    "keywords": [
        "boilerplate",
        "react",
        "typescript-react-boilerplate",
        "typescript"
    ],
    "scripts": {
        "clean": "rimraf dist/*",
        "cleanCov": "rimraf @coverage",
        "start": "npm run dev",
        "dev": "node ./scripts/server/devServer",
        "build": "npm run clean && npm run build:es && npm run build:prod",
        "build:es": "tsc --project ./tsconfig.es.json",
        "build:prod": "cross-env NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.prod.js --progress",
        "test": "jest --config ./scripts/jest/jest.config.js",
        "test:watch": "npm test -- --watch",
        "test:cov": "npm run cleanCov && npm test -- --coverage",
        "test:update": "npm test -- --updateSnapshot"
    },
    "dependencies": {
        "classnames": "^2.2.6",
        "history": "^4.7.2",
        "normalize.css": "^8.0.1",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-router-dom": "^4.3.1"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.7",
        "@types/enzyme": "^3.9.0",
        "@types/jest": "^24.0.9",
        "@types/react": "^16.8.6",
        "@types/react-dom": "^16.8.2",
        "@types/react-router-dom": "^4.3.1",
        "cache-loader": "^2.0.1",
        "chalk": "^2.4.2",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.1",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.10.0",
        "enzyme-to-json": "^3.3.5",
        "file-loader": "^3.0.1",
        "fork-ts-checker-webpack-plugin": "^0.5.2",
        "husky": "^1.3.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.1.0",
        "jest-fetch-mock": "^2.1.1",
        "less": "^3.9.0",
        "less-loader": "^4.1.0",
        "lint-staged": "^8.1.5",
        "mini-css-extract-plugin": "^0.5.0",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
        "react-hot-loader": "^4.7.2",
        "rimraf": "^2.6.3",
        "style-loader": "^0.23.1",
        "terser-webpack-plugin": "^1.2.3",
        "ts-jest": "^24.0.0",
        "ts-loader": "^5.3.3",
        "tslint": "^5.13.1",
        "tslint-react": "^3.6.0",
        "typescript": "^3.3.3333",
        "typings-for-css-modules-loader": "^1.7.0",
        "url-loader": "^1.1.2",
        "webpack": "^4.29.6",
        "webpack-bundle-analyzer": "^3.1.0",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.1"
    },
    "lint-staged": {
        "./src/**/*.tsx?": [
            "tslint",
            "git add"
        ]
    }
}
